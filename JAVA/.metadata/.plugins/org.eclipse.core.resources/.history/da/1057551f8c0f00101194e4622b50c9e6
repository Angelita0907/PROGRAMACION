package controlador;

import modelo.Animal2;
import modelo.ReservaAnimal;

public class GestionaReserva {

	public static void main(String[] args) {
		
		Animal2 a1 = new Animal2(1, "Pepito");
		Animal2 a2 = new Animal2(2, "JC");
		Animal2 a3 = new Animal2(3, "Adrian");
		
		ReservaAnimal r1 = new ReservaAnimal("Reserva 1", "Sevilla", 3000);
		
		r1.addAnimal(a1);
		r1.addAnimal(a2);
		r1.addAnimal(a3);

		r1.mostrarAnimal();

	}

}
