package objetos_beletin2_Ejercicio2;

public class Alumnos {

	/*
	 * nombre: String apellido: String email: String notaP: NotaAsignatura notaB:
	 * NotaAsignatura --> lucia.notaP.primerT / lucia.notaP.nombre
	 * 
	 */
	String nombre;
	String apellido;
	String emai;
	Notas notaP;
	Notas notaBD;

	public Alumnos(String nombre, String emai) {
		super();
		this.nombre = nombre;
		this.emai = emai;
	}

	@Override
	public String toString() {
		return "Alumnos nombre=" + nombre + ", emai=" + emai;
	}

}
