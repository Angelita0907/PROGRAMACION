package orientacion_objetos;

import java.util.Arrays;

import orientacion_objetos_EjemploClase.Ejemplo2Personas;

public class Ejemplo2PersonasEquipo {

	String nombreEquipo;
	int [] puntuaciones;
	Ejemplo2Personas [] personas;
	public Ejemplo2PersonasEquipo(String nombreEquipo, int[] puntuaciones, Ejemplo2Personas[] personas) {
		super();
		this.nombreEquipo = nombreEquipo;
		this.puntuaciones = puntuaciones;
		this.personas = personas;
	}
	@Override
	public String toString() {
		return "Ejemplo2PersonasEquipo [nombreEquipo=" + nombreEquipo + ", puntuaciones="
				+ Arrays.toString(puntuaciones) + ", personas=" + Arrays.toString(personas) + "]";
	}

	
	
}
